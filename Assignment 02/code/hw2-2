#include<iostream>
using namespace std;
int main(){
    int cases=0;
    cin >> cases;
    for(int i=0; i<cases; i++)
    {
        int num=0, sum=0;
        cin >> num;
        for(int j=1; j<num; j++)
        {
            if(num%j==0) sum+=j;
        }
        if(sum==num) cout << "perfect number\n";
        else cout << "non-perfect number\n";
    }
    return 0;
}
